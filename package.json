{
  "name": "uwot",
  "version": "1.0.0-alpha.19",
  "license": "MIT",
  "author": {
    "name": "Chad A. Carino",
    "email": "artist@chadacarino.com",
    "url": "http://www.chadacarino.com"
  },
  "private": true,
  "repository": "github:bangerkuwranger/uwot",
  "scripts": {
    "start": "NODE_ENV=production node ./bin/www",
    "startdev": "NODE_ENV=development node ./bin/www",
    "debug": "NODE_ENV=development nodemon --inspect-brk ./bin/www",
    "debugprod": "NODE_ENV=production nodemon --inspect-brk ./bin/www",
    "test": "nyc --reporter=html --reporter=text npm-run-all -s -c test-unit-core test-unit",
    "test-debug": "nyc --reporter=html --reporter=text npm-run-all -s -c test-unit-core test-unit-debug",
    "test-unit-core": "NODE_ENV=development mocha 'test/helpers.globalSetup.js' --exit",
    "test-unit": "NODE_ENV=development mocha 'test/*.spec.js' --exit",
    "test-unit-debug": "NODE_ENV=development mocha 'test/*.spec.js' --inspect-brk --exit",
    "test-codacy": "npm-run-all test-lcov send-codacy-coverage",
    "test-lcov": "nyc --reporter=lcov --reporter=text npm-run-all -s -c test-unit-core test-unit",
    "send-codacy-coverage": "CTOKEN=$( cat ./codeacy.token ) && cat ./coverage/lcov.info | codacy-coverage -v --token $CTOKEN"
  },
  "dependencies": {
    "bash-ast-traverser": "0.5.0",
    "bash-parser": "0.5.0",
    "bcryptjs": "2.4.3",
    "body-parser": "^1.18.3",
    "codacy-coverage": "^3.4.0",
    "cookie-parser": "^1.4.4",
    "debug": "~2.6.9",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "express-winston": "^3.1.0",
    "favicons": "^5.3.0",
    "fs-extra": "^7.0.1",
    "jquery-touchswipe": "^1.6.19",
    "lodash": "^4.17.11",
    "minimist": "^1.2.0",
    "morgan": "^1.9.1",
    "nconf": "^0.10.0",
    "nedb-core": "^3.0.6",
    "nedb-session-store": "^1.1.2",
    "node-compass": "github:bangerkuwranger/node-compass",
    "node-cron": "^2.0.3",
    "node-timednonce": "^1.1.4",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "password-rules": "^0.0.3",
    "path-is-inside": "^1.0.2",
    "pug": "^2.0.3",
    "remove": "^0.1.5",
    "serve-favicon": "~2.4.5",
    "winston": "^3.2.1"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "devDependencies": {
    "chai": "^4.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.3.0",
    "sinon": "^7.2.7",
    "sinon-chai": "^3.3.0"
  }
}
